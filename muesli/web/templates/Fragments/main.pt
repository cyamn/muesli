<body onload="aprilFool()">

<metal:main metal:use-macro="templates('Fragments/menu.pt').macros['menu']" />

<script>
/* <![CDATA[ */
function set_navigation(nav) {
  var str = "<ul>\n";
  for (var i = 0; i<nav.length; i++) {
    str += '<li><a href="';
    str += nav[i][1];
    str += '">';
    str += nav[i][0];
    str += '</a></li>\n';
  }
  str += '</ul>';
  document.getElementById('history').innerHTML = str;
}
/* ]]> */
</script>

<div id="history">
</div>

<metal:main metal:use-macro="templates('Fragments/message_queue.pt').macros['message_queue']">

<div class="container-fluid">
